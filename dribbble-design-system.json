      },
      "neutrals": {
        "black": "#000000",
        "darkGray": "#0D0C22",
        "mediumGray": "#212133",
        "lightGray": "#F4F4F4",
        "white": "#FFFFFF",
        "offWhite": "#FDFDFD",
        "background": "#F3F3F6"
      },
      "text": {
        "primary": "#0D0C22",
        "secondary": "#212133",
        "tertiary": "#3A3A47",
        "muted": "#6E6D7A"
      },
      "states": {
        "success": "#00B050",
        "warning": "#F4A09C",
        "error": "#E83A30",
        "info": "#1863DC"
      },
      "palette": {
        "pastel": [
          "#FAE6D1", "#FADDD1", "#FAD3D1", "#FAD1E6", "#E5D1FA",
